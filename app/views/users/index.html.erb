<div id="dmain">	
	<%= render :partial=>'layouts/sidebar' %>
	
	<% if rakish? %>
<h1>Listing users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%#= user.password %></td>
    <td><%#= user.encrypted_password %></td>
    <td><%#= user.salt %></td>
    <td><%= user.location %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%#= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />
 <% end %>
<div>
